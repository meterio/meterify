{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "semicolon": [true, "never"],
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "only-arrow-functions": false,
        "interface-name": false,
        "max-line-length":false,
        "no-namespace":false,
        "radix": false
    },
    "rulesDirectory": []
}